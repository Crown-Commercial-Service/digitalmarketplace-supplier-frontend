  "version": "0.0.1",
  "private": true,
  "engine": "node >= 0.10.0",
  "dependencies": {
    "bower": "1.3.12",
    "del": "1.1.1",
    "govuk_frontend_toolkit": "3.5.1",
    "gulp": "3.8.7",
    "gulp-filelog": "0.4.1",
    "gulp-include": "1.1.1",
    "gulp-sass": "1.3.3",
    "gulp-shell": "0.2.9",
